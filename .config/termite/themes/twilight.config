#ocean
[options]
scroll_on_output = false
scroll_on_keystroke = true
audible_bell = false
mouse_autohide = false
allow_bold = false
dynamic_title = true
urgent_on_bell = true
clickable_url = true
font = Droid Sans Mono 9
scrollback_lines = 10000
search_wrap = true
#icon_name = terminal
#geometry = 640x480

# "system", "on" or "off"
cursor_blink = off

# "block", "underline" or "ibeam"
cursor_shape = ibeam

# $BROWSER is used by default

# set size hints for the window
#size_hints = false

# emit escape sequences for other keys modified by Control
#modify_other_keys = false
#

[colors]

# special
foreground      = #a7a7a7
foreground_bold = #a7a7a7
cursor          = #a7a7a7
background      = #1e1e1e

# black
color0  = #1e1e1e
color8  = #5f5a60

# red
color1  = #cf6a4c
color9  = #ff835e

# green
color2  = #8f9d6a
color10 = #e8ffac

# yellow
color3  = #f9ee98
color11 = #fff5aa

# blue
color4  = #7587a6
color12 = #b4cfff

# magenta
color5  = #9b859d
color13 = #fcd8ff

# cyan
color6  = #afc4db
color14 = #cce4ff

# white
color7  = #a7a7a7
color15 = #ffffff

